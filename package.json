{
  "name": "faded-presenter",
  "version": "0.9.7",
  "description": "A simple, light-weight presentation tool to present markdown documents as slide-shows",
  "main": "index.html",
  "scripts": {
    "install-globals": "npm install -g grunt-cli bower",
    "install": "npm run install-globals",
    "postinstall": "bower cache clean && bower install && grunt",
    "develop": "grunt develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/myTerminal/faded-presenter.git"
  },
  "bugs": {
    "url": "https://github.com/myTerminal/faded-presenter/issues"
  },
  "homepage": "https://github.com/myTerminal/faded-presenter#readme",
  "keywords": [
    "markdown",
    "presentation"
  ],
  "author": "Mohammed Ismail Ansari <team.terminal@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "grunt": "~1.0.1",
    "grunt-contrib-watch": "~1.0.0",
    "grunt-contrib-less": "~1.4.0"
  }
}
